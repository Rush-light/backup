
public class test {

	public static void main(String[] args){
		Person taro=new Person();
		taro.name="山田太郎";
		taro.age=20;

		System.out.println(taro.name);
		System.out.println(taro.age);

		Person abc=new Person();
		abc.name="木村次郎";
		abc.age=18;

		System.out.println(abc.name);
		System.out.println(abc.age);
	}
}
